            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/FilesUploadDto"
              }
            }
          }
        },
        "responses": {
          "400": {
            "description": "Bad request, eg too many files or file too large"
          },
          "2XX": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UploadResponseDto"
                }
              }
            }
          }
        },
        "tags": [
          "v1/asset"
        ]
      }
    },
