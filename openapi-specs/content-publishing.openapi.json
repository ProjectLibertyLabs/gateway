  "info": {
    "title": "Content Publishing Service API",
    "description": "Content Publishing Service API",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "FilesUploadDto": {
        "type": "object",
        "properties": {
          "files": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "binary"
            }
          }
        },
        "required": [
          "files"
        ]
      },
      "UploadResponseDto": {
        "type": "object",
        "properties": {
          "assetIds": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "assetIds"
        ]
      },
      "AssetReferenceDto": {
        "type": "object",
        "properties": {
          "referenceId": {
            "type": "string",
            "minLength": 1,
            "description": "The unique Id for the uploaded asset.",
            "example": "bafybeibzj4b4zt4h6n2f6i6lam3cidmywqj5rznb2ofr3gnahurorje2tu"
          },
          "height": {
            "type": "number",
            "description": "A hint as to the rendering height in device-independent pixels for image or video assets.",
            "example": "228"
          },
          "width": {
            "type": "number",
            "description": "A hint as to the rendering width in device-independent pixels for image or video asset",
            "example": "350"
          },
          "duration": {
            "type": "string",
            "pattern": "DURATION_REGEX",
            "description": "Approximate duration of the video or audio asset",
            "example": "PT10M32S"
          }
        },
        "required": [
          "referenceId"
        ]
      },
      "AssetDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "minLength": 1,
            "description": "The display name for the file",
            "example": "TheScream"
          },
          "href": {
            "type": "string",
            "minLength": 1,
            "description": "The URL for the given content",
            "example": "https://upload.wikimedia.org/wikipedia/commons/d/d9/Wilhelm_Scream.ogg"
          },
          "references": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AssetReferenceDto"
            }
          }
        }
      },
      "TagDto": {
        "type": "object",
        "properties": {
          "type": {
            "enum": [
              "mention",
              "hashtag"
            ],
            "type": "string",
            "description": "Identifies the tag type",
            "example": "mention"
          },
          "name": {
            "type": "string",
            "minLength": 1,
            "description": "The text of the tag",
            "example": "@sally"
          },
          "mentionedId": {
            "type": "string",
            "description": "Link to the user mentioned",
            "example": "dsnp://12345678"
          }
        },
        "required": [
          "type"
        ]
      },
      "LocationDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "minLength": 1,
            "description": "The display name for the location",
            "example": "New York City, NY"
          },
          "accuracy": {
            "type": "number",
            "minimum": 0,
            "maximum": 100,
            "description": "The accuracy of the coordinates as a percentage.  (e.g. 94.0 means 94.0% accurate)",
            "example": "94.0"
          },
          "altitude": {
            "type": "number",
            "description": "The altitude of the location",
            "example": "10"
          },
          "latitude": {
            "type": "number",
            "description": "The latitude of the location",
            "example": "40.73"
          },
          "longitude": {
            "type": "number",
            "description": "The longitude of the location",
            "example": "-73.93"
          },
          "radius": {
            "type": "number",
            "minimum": 0,
            "description": "The area around the given point that comprises the location",
            "example": "100"
          },
          "units": {
            "enum": [
              "cm",
              "m",
              "km",
              "inches",
              "feet",
              "miles"
            ],
            "type": "string",
            "description": "The units for radius and altitude (defaults to meters)",
            "example": "m"
          }
        },
        "required": [
          "name"
        ]
      },
      "NoteActivityDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "The display name for the activity type",
            "example": "A simple activity"
          },
          "content": {
            "type": "string",
            "minLength": 1,
            "description": "Text content of the note",
            "example": "Hello world!"
          },
          "published": {
            "type": "string",
            "description": "The time of publishing ISO8601",
            "example": "1970-01-01T00:00:00+00:00"
          },
          "assets": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AssetDto"
            }
          },
          "tag": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TagDto"
            }
          },
          "location": {
            "$ref": "#/components/schemas/LocationDto"
          }
        },
        "required": [
          "content",
          "published"
        ]
      },
      "BroadcastDto": {
        "type": "object",
        "properties": {
          "content": {
            "$ref": "#/components/schemas/NoteActivityDto"
          }
        },
        "required": [
          "content"
        ]
      },
      "AnnouncementResponseDto": {
        "type": "object",
        "properties": {
          "referenceId": {
            "type": "string"
          }
        },
        "required": [
          "referenceId"
        ]
      },
      "ReplyDto": {
        "type": "object",
        "properties": {
          "inReplyTo": {
            "type": "string",
            "description": "Target DSNP Content URI",
            "example": "dsnp://78187493520/bdyqdua4t4pxgy37mdmjyqv3dejp5betyqsznimpneyujsur23yubzna"
          },
          "content": {
            "$ref": "#/components/schemas/NoteActivityDto"
          }
        },
        "required": [
          "inReplyTo",
          "content"
        ]
      },
      "ReactionDto": {
        "type": "object",
        "properties": {
          "emoji": {
            "type": "string",
            "minLength": 1,
            "pattern": "DSNP_EMOJI_REGEX",
            "description": "the encoded reaction emoji",
            "example": "😀"
          },
          "apply": {
            "type": "number",
            "description": "Indicates whether the emoji should be applied and if so, at what strength",
            "example": "1"
          },
          "inReplyTo": {
            "type": "string",
            "description": "Target DSNP Content URI",
            "example": "dsnp://78187493520/bdyqdua4t4pxgy37mdmjyqv3dejp5betyqsznimpneyujsur23yubzna"
          }
        },
        "required": [
          "emoji",
          "apply",
          "inReplyTo"
        ]
      },
      "UpdateDto": {
        "type": "object",
        "properties": {
          "targetContentHash": {
            "type": "string",
            "description": "Target DSNP Content Hash",
            "example": "bdyqdua4t4pxgy37mdmjyqv3dejp5betyqsznimpneyujsur23yubzna"
          },
          "targetAnnouncementType": {
            "enum": [
              "broadcast",
              "reply"
            ],
            "type": "string",
            "description": "Target announcement type",
            "example": "broadcast"
          },
          "content": {
            "$ref": "#/components/schemas/NoteActivityDto"
          }
        },
        "required": [
          "targetContentHash",
          "targetAnnouncementType",
          "content"
        ]
      },
      "TombstoneDto": {
        "type": "object",
        "properties": {
          "targetContentHash": {
            "type": "string",
            "description": "Target DSNP Content Hash",
            "example": "bdyqdua4t4pxgy37mdmjyqv3dejp5betyqsznimpneyujsur23yubzna"
          },
          "targetAnnouncementType": {
            "enum": [
              "broadcast",
              "reply"
            ],
            "type": "string",
            "description": "Target announcement type",
            "example": "broadcast"
          }
        },
        "required": [
          "targetContentHash",
          "targetAnnouncementType"
        ]
      },
      "ProfileActivityDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "The display name for the activity type",
            "example": "A simple activity"
          },
          "summary": {
            "type": "string",
            "description": "Used as a plain text biography of the profile",
            "example": "John Doe is actually a small kitten."
          },
          "published": {
            "type": "string",
            "description": "The time of publishing ISO8601",
            "example": "1970-01-01T00:00:00+00:00"
          },
          "icon": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AssetReferenceDto"
            }
          },
          "tag": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TagDto"
            }
          },
          "location": {
            "$ref": "#/components/schemas/LocationDto"
          }
        }
      },
      "ProfileDto": {
        "type": "object",
        "properties": {
          "profile": {
            "$ref": "#/components/schemas/ProfileActivityDto"
          }
        },
        "required": [
          "profile"
        ]
      }
    }
  }
}