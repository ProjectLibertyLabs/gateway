    "@projectlibertylabs/graph-sdk": "2.0.1",
    "@projectlibertylabs/siwfv1": "npm:@projectlibertylabs/siwf@1.1.7",
    "@projectlibertylabs/siwfv2": "npm:@projectlibertylabs/siwf@2.3.4",
    "@songkeys/nestjs-redis": "11.0.0",
    "@types/express": "5.0.3",
    "@types/multer": "2.0.0",
    "@willsoto/nestjs-prometheus": "6.0.2",
    "axios": "1.11.0",
    "bullmq": "5.58.2",
