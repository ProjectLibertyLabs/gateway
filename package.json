{
  "name": "@projectlibertylabs/gateway",
  "version": "1.0.0",
  "description": "Monorepo for Gateway shared packages",
  "author": "Project Liberty, LLC",
  "license": "Apache-2.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:account-api": "nest build account-api",
    "build:account-worker": "nest build account-worker",
    "build:content-publishing-api": "nest build content-publishing-api",
    "build:content-publishing-worker": "nest build content-publishing-worker",
    "build:content-watcher": "nest build content-watcher",
    "build:graph-api": "nest build graph-api",
    "build:graph-worker": "nest build graph-worker",
    "build": "npm run build account-api ; npm run build account-worker ; npm run build content-publishing-api ; npm run build content-publishing-worker ; npm run build content-watcher ; npm run build graph-api ; npm run build graph-worker",
    "start:account-api": "nest start account-api",
    "start:account-worker": "nest start account-worker",
    "start:content-publishing-api": "nest start content-publishing-api",
    "start:content-publishing-worker": "nest start content-publishing-worker",
    "start:content-watcher": "nest start content-watcher",
    "start:graph-api": "nest start graph-api",
    "start:graph-worker": "nest start graph-worker",
    "generate:metadata:account": "npx ts-node -r tsconfig-paths/register apps/account-api/src/generate-metadata.ts",
    "generate:metadata:content-publishing": "npx ts-node -r tsconfig-paths/register apps/content-publishing-api/src/generate-metadata.ts",
    "generate:metadata:content-watcher": "npx ts-node -r tsconfig-paths/register apps/content-watcher/src/generate-metadata.ts",
    "generate:metadata:graph": "npx ts-node -r tsconfig-paths/register apps/graph-api/src/generate-metadata.ts",
    "generate:metadata": "npm run generate:metadata:account ; npm run generate:metadata:content-publishing ; npm run generate:metadata:content-watcher ; npm run generate:metadata:graph",
    "generate:openapi:account": "nest start account --entryFile build-openapi",
    "generate:openapi:content-publishing": "nest start content-publishing-api --entryFile build-openapi",
    "generate:openapi:content-watcher": "nest start content-watcher --entryFile build-openapi",
    "generate:openapi:graph": "nest start graph-api --entryFile build-openapi",
    "generate:openapi": "npm run generate:openapi:account ; npm run generate:openapi:content-publishing ; npm run generate:openapi:content-watcher ; npm run generate:openapi:graph",
    "generate:types:graph": "npx openapi-client-axios-typegen openapi-specs/graph-webhooks.openapi.yaml > libs/graph-lib/src/types/webhook-types.d.ts ; prettier --write libs/graph-lib/src/types/webhook-types.d.ts",
    "generate:types:content-watcher": "npx @hey-api/openapi-ts --types -i openapi-specs/content-announcement.openapi.json -o libs/content-watcher-lib/src/types/content-announcement ; prettier --write libs/content-watcher-lib/src/types/content-announcement",
    "generate:types": "npm run generate:types:graph ; npm run generate:types:content-watcher",
    "generate:swagger-ui:account": "npx --yes @redocly/cli build-docs openapi-specs/account.openapi.json --output=./docs/account/index.html",
    "generate:swagger-ui:content-publishing": "npx --yes @redocly/cli build-docs openapi-specs/content-publishing.openapi.json --output=./docs/content-publishing/index.html",
    "generate:swagger-ui:content-watcher": "npx --yes @redocly/cli build-docs openapi-specs/content-watcher.openapi.json --output=./docs/content-watcher/index.html",
    "generate:swagger-ui:graph": "npx --yes @redocly/cli build-docs openapi-specs/graph.openapi.json --output=./docs/graph/index.html",
    "generate:swagger-ui": "npm run generate:swagger-ui:account ; npm run generate:swagger-ui:content-publishing ; npm run generate:swagger-ui:content-watcher ; npm run generate:swagger-ui:graph",
    "test": "jest",
    "test:verbose": "jest --coverage --verbose",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@bull-board/api": "^5.21.4",
    "@bull-board/express": "^5.21.4",
    "@bull-board/nestjs": "^5.21.4",
    "@bull-board/ui": "^5.21.4",
    "@dsnp/activity-content": "^1.1.0",
    "@dsnp/frequency-schemas": "^1.1.0",
    "@dsnp/graph-sdk": "^1.1.4",
    "@dsnp/parquetjs": "^1.7.0",
    "@frequency-chain/api-augment": "^1.11.1",
    "@multiformats/blake2": "^1.0.13",
    "@nestjs/bullmq": "^10.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/microservices": "^10.4.1",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@polkadot/api": "^10.9.1",
    "@polkadot/api-base": "^10.9.1",
    "@polkadot/keyring": "^12.3.2",
    "@polkadot/types": "^10.9.1",
    "@polkadot/util": "^12.3.2",
    "@polkadot/util-crypto": "^12.3.2",
    "@projectlibertylabs/siwf": "^1.1.2",
    "@songkeys/nestjs-redis": "^10.0.0",
    "@types/multer": "^1.4.12",
    "app": "^0.1.0",
    "axios": "^1.7.5",
    "bullmq": "^5.12.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "form-data": "^4.0.0",
    "ioredis": "^5.4.1",
    "ipfs-only-hash": "^4.0.0",
    "joi": "^17.13.1",
    "mime-types": "^2.1.35",
    "multiformats": "^9.9.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "time-constants": "^1.0.3"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.45.1",
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.1",
    "@projectlibertylabs/frequency-scenario-template": "^1.1.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.5.0",
    "openapi-client-axios-typegen": "^7.6.2",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
